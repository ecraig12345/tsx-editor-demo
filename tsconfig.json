{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "lib",
    "target": "esnext",
    "module": "esnext",
    "jsx": "react",
    "noUnusedLocals": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "preserveConstEnums": true
  },
  "include": [
    "src"
  ]
}